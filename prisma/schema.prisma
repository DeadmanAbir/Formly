generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id     String  @id @default(uuid())
  email  String  @unique
  forms  Form[]  @relation
}

model Form {
  id        String @id
  content   String
  userId    String
  user      User   @relation(fields: [userId], references: [id], onDelete: Cascade)
}